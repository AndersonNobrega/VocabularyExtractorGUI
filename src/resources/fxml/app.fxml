<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="550.0" prefWidth="900.0" stylesheets="@../css/appStylesheet.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.bluebird.UserInterface.App.AppController">
   <children>
      <Button fx:id="dirBrowser" layoutX="665.0" layoutY="330.0" mnemonicParsing="false" onAction="#browseProjectDirectory" text="Browse" />
      <Label layoutX="40.0" layoutY="155.0" text="Project Name" />
      <Label layoutX="40.0" layoutY="245.0" text="Project Revision" />
      <Label layoutX="40.0" layoutY="335.0" text="Project Path" />
      <Label layoutX="40.0" layoutY="425.0" text="Vxl Path" />
      <Label layoutX="780.0" layoutY="525.0" text="Alpha Version" />
      <Button fx:id="vxlBrowser" layoutX="665.0" layoutY="420.0" mnemonicParsing="false" onAction="#vxlLocation" text="Browse" />
      <TextField fx:id="projectName" layoutX="160.0" layoutY="150.0" prefHeight="25.0" prefWidth="250.0" />
      <TextField fx:id="projectRevision" layoutX="160.0" layoutY="240.0" prefHeight="25.0" prefWidth="250.0" />
      <MenuBar prefHeight="30.0" prefWidth="900.0">
        <menus>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#openGitHub" text="GitHub" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Label fx:id="filePath" layoutX="160.0" layoutY="330.0" prefHeight="25.0" prefWidth="500.0" />
      <Button layoutX="388.5" layoutY="505.0" mnemonicParsing="false" onAction="#extract" prefHeight="25.0" prefWidth="125.0" text="Extract" />
      <Label fx:id="vxlDirPath" layoutX="160.0" layoutY="420.0" prefHeight="25.0" prefWidth="500.0" />
      <Label fx:id="nameStatus" layoutX="450.0" layoutY="155.0" prefHeight="15.0" prefWidth="200.0" />
      <Label fx:id="revisionStatus" layoutX="450.0" layoutY="245.0" prefHeight="15.0" prefWidth="200.0" />
      <Label fx:id="dirPathStatus" layoutX="750.0" layoutY="335.0" prefWidth="125.0" />
      <Label fx:id="vxlPathStatus" layoutX="750.0" layoutY="425.0" prefWidth="125.0" />
      <Label layoutX="40.0" layoutY="65.0" text="Language" />
      <ComboBox fx:id="languageComboBox" layoutX="160.0" layoutY="60.0" prefWidth="120.0" promptText="Language" />
      <Label fx:id="languageStatus" layoutX="335.0" layoutY="65.0" prefHeight="15.0" prefWidth="200.0" />
   </children>
</AnchorPane>
